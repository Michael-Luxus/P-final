            <div id="message"></div>
            <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
            <%= f.email_field :email, autocomplete: "email", id:"email", placeholder:"Addresse email..." %>
            <%= f.password_field :password, autocomplete: "current-password", id:"pass", type:"password", placeholder:"Votre mot de passe..." %>
            <%= link_to "Mot de passe oublié?", new_password_path(resource_name), class:"oublie" %>
            <div><input type="submit" name="send" class="submitBnt" value="connecter"></div>
            <% if devise_mapping.rememberable? -%>
            <div class="cent">
              <div class="ele">
                <li><%= f.check_box :remember_me, class:"check" %></li>
                <li><p>Se souvenir de moi</p></li>
              </div>
            </div>
            <% end %>
            <% end %>
